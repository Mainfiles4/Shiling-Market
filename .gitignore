# Ignore system files and directories
/system/
/writable/

# Ignore composer dependencies
/vendor/

# Ignore environment-specific configuration files
.env

# Ignore user-specific IDE or editor files
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Ignore logs and temporary files
/app/Logs/
/app/Views/*.php
/tests/_output/
/tests/report/

# Ignore application-specific cache and session files
/app/Cache/
/app/Session/

# Ignore any built assets or compiled files
/public/assets/

# Ignore any user-generated files or directories
/uploads/
